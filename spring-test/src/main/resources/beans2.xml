<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="person" name="pe,pee" class="com.test.Person" scope="singleton" >
		<constructor-arg name="id"  value="1" index="0"></constructor-arg>
		<constructor-arg name="name" value="tom"></constructor-arg>
		<constructor-arg name="birthday" value="2020-07-30"></constructor-arg>
		<constructor-arg name="age" value="1"></constructor-arg>
		<property name="id" value="1"/>
		<property name="name" value="Java"/>
		<property name="age" value="17"/>
		<property name="birthday" value="2020-07-30"/>
	</bean>

	<alias name="person" alias="person4,person5"/>

</beans>
